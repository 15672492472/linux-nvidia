/dts-v1/;

/memreserve/ 0x80000000 0x00010000;

#include "tegra194-soc/tegra194-soc-base.dtsi"
#include "tegra194-soc/tegra194-cpus.dtsi"
#include "tegra194-platforms/tegra194-sim-dp.dtsi"
#include "tegra194-soc/tegra194-soc-sdhci.dtsi"
#include "tegra194-soc/tegra194-soc-prod.dtsi"
/ {
	model = "t186ref";
	compatible = "nvidia,tegra186";

	firmware {
		android {
			compatible = "android,firmware";
			hardware = "t186ref";
		};
	};

	chosen {
		bootargs ="mem=1024M@2048M console=ttyS0,115200 earlyprintk=uart8250-32bit,0x03100000";
		stdout-path = &uarta;
	};

	memory@80000000 {
		device_type = "memory";
		reg = < 0x0 0x80000000 0x0 0xc0000000 >;
	};

	interrupt-controller@3881000 {
		status = "okay";
	};

	interrupt-controller@3000000 {
		status = "okay";
	};

	timer {
		status = "okay";
	};

	serial@3100000 {
		status = "okay";
		clock-frequency = <408000000>;
	};

	chipid@100000 {
		status = "okay";
	};

	iommu@12000000 {
		status = "okay";
	};

	reserved-memory {
		generic_carveout {
			size = <0 0x30000000>;
			status = "okay";
		};
	};

	tegra-carveouts {
		status = "okay";
	};

	host1x {
		status = "okay";

		nvdisplay@15200000 {
			status = "okay";
			nvidia,fb-bpp = <0x20>;
			nvidia,dc-flags = <TEGRA_DC_FLAG_ENABLED>;
			nvidia,fb-win = <0x0>;
			win-mask = <0x7>;
			nvidia,dc-ctrlnum = <0x0>;
			nvidia,dc-or-node = "/host1x/sor";
		};

		sor {
			status = "okay";
		};

		dpaux@155c0000 {
			status = "okay";
		};
	};

	ethernet@03b40000 {
		compatible = "smsc,lan91c111";
		reg = <0x0 0x03b40000 0x0 0x00000100>;
		interrupts = <0 218 0x4>;
		reg-io-width = <4>;
		smsc,irq-active-high;
		smsc,irq-push-pull;
	};
	sdhci@3460000 {
		       nvidia,enable-strobe-mode;
		       max-clk-limit = <6400000>;
		       status = "okay";
	};
};
