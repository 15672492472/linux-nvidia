#include "tegra186-soc/tegra186-soc-vcm-prod.dtsi"

/ {

	pinctrl@3520000 {
		nvidia,sata-use-prods;
		prod-settings {
			#prod-cells = <4>;
			mask-one-style;
			prod_c_utmi0 {
				board {
					prod = <0 0x94 0x000001cf 0x00000000>;
				};
			};
			prod_c_utmi2 {
				board {
					prod = <0 0x114 0x000001cf 0x00000000>;
				};
			};
			prod_c_sata {
				board {
					prod = <5 0x80 0x3f3f3f3f 0x23271f19
						5 0x00 0x03000000 0x0
						5 0x8c 0x003f003f 0x000a0007>;
				};
			};
		};
	};
	ahci-sata@3507000 {
		prod-settings {
			mask-one-style;
			prod {
				board {
					prod = <
						1 0x00000680 0x00000001 0x1
						1 0x00000690 0x3FFFFFFF 0x00000715	// T_SATA0_CHX_PHY_CTRL1_GEN1	31:0
						1 0x00000694 0x3FFFFFFF 0x0000A018	// T_SATA0_CHX_PHY_CTRL1_GEN2	31:0
						1 0x00000680 0x00000001 0x0
					>;
				};
			};
		};
	};
};
