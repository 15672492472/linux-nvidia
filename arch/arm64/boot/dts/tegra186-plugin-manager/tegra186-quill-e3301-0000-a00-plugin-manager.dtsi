#include "tegra186-quill-odm-data-plugin-manager.dtsi"

/ {
	eeprom-manager {
		data-size = <0x100>;
		boardid-with-revision = <3301>;
		bus@0 {
			i2c-bus = <&gen8_i2c>;
			word-address-1-byte-slave-addresses = <0x50 0x57>;
			eeprom@0 {
				slave-address = <0x50>;
				label = "cvm";
			};
			eeprom@1 {
				slave-address = <0x57>;
				label = "cvb";
			};
		};
		bus@1 {
			i2c-bus = <&gen2_i2c>;
			word-address-1-byte-slave-addresses = <0x51>;
			eeprom@0 {
				slave-address = <0x51>;
			};
		};
		bus@2 {
			i2c-bus = <&gen1_i2c>;
			word-address-1-byte-slave-addresses = <0x50>;
			eeprom@0 {
				slave-address = <0x50>;
			};
		};
		bus@3 {
			i2c-bus = <&gen7_i2c>;
			eeprom@0 {
				slave-address = <0x54>;
				label = "cam";
				enable-gpio = <&gpio_i2c_0_77 9>;
			};
		};
	};
	plugin-manager {
		fragement-e3301-a01@1 {
			ids = "3301-1000-100";
			override@0 {
				target = <&ina3221x_41>;
				_overlay_ {
					channel@1 {
						ti,shunt-resistor-mohm = <10>;
					};
				};
			};
		};

		e3301-a02-char@0 {
			ids = ">=3301-1080-200";
			override@0 {
				target = <&spmic_ldo8>;
				_overlay_ {
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <3300000>;
					regulator-always-on;
					regulator-boot-on;
					status = "okay";
				};
			};
			override@1 {
				target = <&tegra_pmc_iopower>;
				_overlay_ {
					iopower-dmic-hv-supply =  <&spmic_ldo8>;
					iopower-audio-hv-supply = <&spmic_ldo8>;
				};
			};
		};

		e3301-a02-slt-char@0 {
			ids = ">=3301-1080-200", ">=3301-1099-200";
			override@0 {
				target = <&enable_fan_tach>;
				_overlay_ {
					status = "okay";
				};
			};
			override@1 {
				target = <&tegra_pwm5>;
				_overlay_ {
					status = "okay";
				};
			};
		};

		e3301-a02-all@1 {
			ids = ">=3301-1000-200", ">=3301-1080-200";
			override@1 {
				target = <&{/host1x/sor}>;
				_overlay_ {
					status = "okay";
					hdmi-display {
						status = "okay";
					};
				};
			};
			override@2 {
				target = <&{/host1x/sor1}>;
				_overlay_ {
					status = "disabled";
					hdmi-display {
						status = "disabled";
					};
				};
			};
			override@3 {
				target = <&{/host1x/nvdisplay@15210000}>;
				_overlay_ {
					nvidia,dc-or-node = "/host1x/sor";
				};
			};
		};
	};
};
