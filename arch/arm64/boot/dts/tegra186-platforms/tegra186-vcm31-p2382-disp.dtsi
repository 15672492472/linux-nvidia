			ti,pll-refclk-cfg = <0x4>;
			ti,dsi-cha-clk-range = <0x59>;
			ti,disable-assr = <1>;
			ti,dp-ssc-cfg = <0x30>;
			ti,h-pulse-width-low = <0x2c>;
			ti,h-pulse-width-high = <0x80>;
			ti,v-pulse-width-low = <0x05>;
