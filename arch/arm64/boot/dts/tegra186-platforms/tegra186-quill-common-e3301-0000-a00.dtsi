#include "tegra186-quill-common.dtsi"
#include "tegra186-quill-e3301-powermon.dtsi"
#include "tegra186-quill-pb-e3301-0000-a00-pinmux-manual.dtsi"
#include "tegra186-quill-pb-thermal.dtsi"

/ {
        model = "quill-3301";
        compatible = "nvidia,quill-3301", "nvidia,tegra186";

        #address-cells = <2>;
        #size-cells = <2>;

	pmc@c360000 {
		io-pad-defaults {
			sdmmc1-hv {
				nvidia,enable-dynamic-pad-voltage;
			};
			sdmmc2-hv {
				nvidia,enable-dynamic-pad-voltage;
			};
			sdmmc3-hv {
				nvidia,enable-dynamic-pad-voltage;
			};
		};
	};

	spi@3210000 {
		status = "okay";
	};

	spi@3230000 {
		status = "okay";
	};

	spi@3240000 {
		status = "okay";
	};

	ahci-sata@3507000 {
		status = "okay";
	};

	xudc@3550000 {
		status = "okay";
		phys = <&padctl_uphy TEGRA_PADCTL_UPHY_UTMI_P(0)>,
			<&padctl_uphy TEGRA_PADCTL_UPHY_USB3_P(1)>;
		phy-names = "usb2", "usb3";
	};

	xhci@3530000 {
		status = "okay";
		phys = <&padctl_uphy TEGRA_PADCTL_UPHY_UTMI_P(2)>,
			<&padctl_uphy TEGRA_PADCTL_UPHY_USB3_P(2)>,
			<&padctl_uphy TEGRA_PADCTL_UPHY_HSIC_P(0)>;
		phy-names = "utmi-2", "usb3-2", "hsic-0";
	};

	external-connection {
		vbus_id_extcon: extcon@1 {
			compatible = "extcon-gpio-states";
			reg = <0x1>;
			extcon-gpio,name = "VBUS";
			extcon-gpio,cable-states = <0x3 0x0
						    0x0 0x2
						    0x1 0x2
						    0x2 0x1>;
			gpios = <&tegra_gpio TEGRA_GPIO(FF, 4) 0 &spmic 0 0>;
			extcon-gpio,out-cable-names = "USB", "USB-Host";
			#extcon-cells = <1>;
		};
	};

	host1x {
		sor1 {
                        hdmi-display {
                                disp-default-out {
                                        nvidia,out-flags = <TEGRA_DC_OUT_HOTPLUG_HIGH>;
				};
			};
		};
		sor {
                        hdmi-display {
                                disp-default-out {
                                        nvidia,out-flags = <TEGRA_DC_OUT_HOTPLUG_HIGH>;
				};
			};
		};
	};
};

