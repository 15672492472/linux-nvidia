#include "tegra186-platforms/tegra186-vcm31-p2382-common.dtsi"
#include "tegra186-platforms/tegra186-vcm31-p2382-disp.dtsi"

/ {
	nvidia,dtsfilename = __FILE__;
	nvidia,boardids = "p2382:0010:A01";
	nvidia,proc-boardid = "p2382:0010:A01";

	tegra_hv: hyp {
		compatible = "nvidia,tegra-hv";
		status = "okay";
	};

	chosen {
		nvidia,tegra-hypervisor-mode;
	};

	bpmp {
		compatible = "nvidia,tegra186-bpmp-hv";
		ivc_queue = <&tegra_hv 0 14>;
	};

	bpmp_i2c {
                spmic: spmic@3c {
			/delete-property/ maxim,enable-sleep;
			/delete-node/ backup-battery;
		};
	};

	aon@c160000 {
		status = "disabled";
	};

	cpuidle {
		compatible = "nvidia,tegra18x-cpuidle";
		status = "disabled";
	};

	i2c@3160000 {
		compatible = "nvidia,tegra186-i2c-hv";
		ivc_queue = <&tegra_hv 33>;
		tegra_nct72: onsemi,nct72@4c {
			status = "okay";
			/delete-property/ interrupt-parent;
			/delete-property/ interrupts;
			/delete-property/ temp-alert-gpio;
		};
	};

	i2c@3180000 {
		compatible = "nvidia,tegra186-i2c-hv";
		ivc_queue = <&tegra_hv 33>;
	};

	i2c@3190000 {
		compatible = "nvidia,tegra186-i2c-hv";
		ivc_queue = <&tegra_hv 33>;
	};

	i2c@31b0000 {
		compatible = "nvidia,tegra186-i2c-hv";
		ivc_queue = <&tegra_hv 33>;
	};

	i2c@31c0000 {
		compatible = "nvidia,tegra186-i2c-hv";
		ivc_queue = <&tegra_hv 33>;
	};

	i2c@31e0000 {
		compatible = "nvidia,tegra186-i2c-hv";
		ivc_queue = <&tegra_hv 33>;
	};

	i2c@c240000 {
		compatible = "nvidia,tegra186-i2c-hv";
		ivc_queue = <&tegra_hv 33>;
	};

	i2c@c250000 {
		compatible = "nvidia,tegra186-i2c-hv";
		ivc_queue = <&tegra_hv 33>;
	};

	mc_sid@2c00000 {
		nvidia,by-pass-smmu-streamid = <0x7e>;
		status = "okay";
	};

	pmc@c370000 {
		status = "disabled";
	};

	tegra_hv_wdt {
		compatible = "nvidia,tegra-hv-wdt";
		status = "okay";
		ivc = <&tegra_hv 32>;
	};

	host1x: host1x {
		reg = <0x0 0x13e10000 0x0 0x00010000>;
		interrupts = <0 265 0x04
					0 263 0x04>;
		nvidia,vmid = <1>;

		nvidia,ch-base = <0>;
		nvidia,nb-channels = <63>;

		nvidia,nb-hw-pts = <576>;
		nvidia,pts-base = <0>;
		nvidia,nb-pts = <576>;

		vmserver-owns-engines = <1>;

		ivc-queue0 = <&tegra_hv 28>;

		vic@15340000 {
			compatible = "nvidia,tegra186-vhost-vic";
			status = "okay";
		};

		nvenc@154c0000 {
			compatible = "nvidia,tegra186-vhost-nvenc";
			status = "okay";
		};

		nvdec@15480000 {
			compatible = "nvidia,tegra186-vhost-nvdec";
			status = "okay";
		};

		nvjpg@15380000 {
			compatible = "nvidia,tegra186-vhost-nvjpg";
			status = "okay";
		};

		ctx0 {
			 status = "okay";
		};

		ctx1 {
			 status = "okay";
		};

		ctx2 {
			 status = "okay";
		};

		ctx3 {
			 status = "okay";
		};

		ctx4 {
			status = "okay";
		};

		ctx5 {
			status = "okay";
		};

		ctx6 {
			status = "okay";
		};

		ctx7 {
			status = "okay";
		};
	};

	generic-system-config {
		status = "dislabled";
	};

	dma@2600000 {
		dma-channels = <32>;
		nvidia,start-dma-channel-index = <0>;
		nvidia,stream-id = <TEGRA_SID_GPCDMA_0>;
		status = "okay";
	};

	intc: interrupt-controller@3881000 {
		reg = <0x0 0x03881000 0x0 0x00001000
		       0x0 0x03892000 0x0 0x00002000>;
	};

	gp10b {
		status = "disabled";
	};

	vgpu {
		compatible = "nvidia,tegra186-gp10b-vgpu", "nvidia,tegra124-gk20a-vgpu";
		reg = <0x0 0x18000000 0x0 0x1000000>;
		nvidia,host1x = <&host1x>;
		ivc-queue3 = <&tegra_hv 30>;
		ivc-queue4 = <&tegra_hv 31>;
		mempool3 = <&tegra_hv 0>;
	};

	thermal-zones {
		BCPU-therm {
			status = "disabled";
		};

		MCPU-therm {
			status = "disabled";
		};

		GPU-therm {
			status = "disabled";
		};

		PLL-therm {
			status = "disabled";
		};

		AO-therm {
			status = "disabled";
		};

		Tboard_tegra {
			status = "disabled";
		};

		Tdiode_tegra {
			status = "disabled";
		};
	};
	sound {
		status = "disabled";
	};

	sound_ref {
		status = "disabled";
	};

	ahub {
		status = "disabled";
	};

        adsp_audio {
                status = "disabled";
        };

        adsp@2993000 {
                status = "disabled";
        };
};
